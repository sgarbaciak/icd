{
  "name": "icd",
  "version": "0.1.0",
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "express-session": "^1.15.2",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.2",
    "rest": "^2.0.0",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "nodemon": "^1.11.0",
    "webpack": "^2.5.1"
  },
  "scripts": {
    "build-front": "node ./node_modules/webpack/bin/webpack.js",
    "start": "babel-node ./server/server.js --presets es2015",
    "watch-back": "nodemon ./server/server.js --exec babel-node --presets es2015",
    "watch-front": "webpack --watch",
    "dev": "concurrently --kill-others \"npm run watch-back\" \"npm run watch-front\""
